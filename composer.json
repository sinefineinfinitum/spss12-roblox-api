{
  "name": "sinefineinfinitum/wp-roblox-api",
  "description": "ROblox API Integration",
  "minimum-stability": "stable",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "spss12"
    }
  ],
  "require": {
    "php": ">=8.0",
    "php-di/php-di": "^7.1",
    "psr/log": "*",
    "campo/random-user-agent": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "SineFine\\RobloxApi\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpstan/extension-installer": "^1.4",
    "szepeviktor/phpstan-wordpress": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
